generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  // ‚ùå no url here
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  createdAt DateTime @default(now())
}

model Practical {
  id          Int      @id @default(autoincrement())
  title       String
  description String?
  fileUrl     String?
  createdAt   DateTime @default(now())
}
